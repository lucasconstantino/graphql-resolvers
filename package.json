    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-spies": "^0.7.1",
    "codecov": "^2.1.0",
    "eslint": "^3.10.2",
    "eslint-config-taller": "^1.0.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "graphql": "^0.9.2",
    "graphql-tools": "^0.11.0",
